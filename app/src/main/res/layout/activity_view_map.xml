<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.widget.ViewMapTitleActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_progress"
            style="@style/style_item_selector"
            android:text="progress" />

        <TextView
            android:id="@+id/tv_multiple_list_view"
            style="@style/style_item_selector"
            android:text="多列的listView" />

        <TextView
            android:id="@+id/tv_custom_text"
            style="@style/style_item_selector"
            android:text="自定义文字" />

        <TextView
            android:id="@+id/tv_custom_round"
            style="@style/style_item_selector"
            android:text="@string/custom_round" />

        <TextView
            android:id="@+id/tv_custom_random"
            style="@style/style_item_selector"
            android:text="自定义随机布局" />

        <TextView
            android:id="@+id/tv_custom_left_and_right"
            style="@style/style_item_selector"
            android:text="自定义左右布局" />

        <TextView
            android:id="@+id/tv_custom_progress"
            style="@style/style_item_selector"
            android:text="自定义圆圈进度条" />

        <TextView
            android:id="@+id/tv_custom_touch"
            style="@style/style_item_selector"
            android:text="自定义触摸解锁" />

        <TextView
            android:id="@+id/tv_custom_input_password"
            style="@style/style_item_selector"
            android:text="自定义密码输入框" />

        <TextView
            android:id="@+id/tv_scroll_view"
            style="@style/style_item_selector"
            android:text="自定义滑动 控件" />

        <TextView
            android:id="@+id/tv_page_view"
            style="@style/style_item_selector"
            android:text="自定义滑动的PageView" />

        <TextView
            android:id="@+id/tv_test_hm"
            style="@style/style_item_selector"
            android:text="自定义滑动按钮和水波纹效果" />

        <TextView
            android:id="@+id/tv_custom_menu"
            style="@style/style_item_selector"
            android:text="自定义侧滑的布局" />

        <TextView
            android:id="@+id/tv_custom_name_list"
            style="@style/style_item_selector"
            android:text="自定义名字的首字母列表" />

    </LinearLayout>

</androidx.core.widget.NestedScrollView>