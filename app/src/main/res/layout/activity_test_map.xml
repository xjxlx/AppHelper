<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".test.TestMapActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_test_handler"
            style="@style/style_todo_item"
            android:text="测试Handler" />

        <TextView
            android:id="@+id/tv_elv"
            style="@style/style_todo_item"
            android:text="测试ExpandableListView加载顺序" />

        <TextView
            android:id="@+id/tv_flex_box"
            style="@style/style_todo_item"
            android:text="测试FlexboxLayout" />

        <TextView
            android:id="@+id/tv_test_js_to_android"
            style="@style/style_todo_item"
            android:text="Android和JS互相调用" />

    </LinearLayout>

</androidx.core.widget.NestedScrollView>