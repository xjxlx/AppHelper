<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.jetpack.JetPackMapActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_lifecycle"
            style="@style/style_todo_item"
            android:text="Lifecycle" />

        <TextView
            android:id="@+id/tv_view_model"
            style="@style/style_todo_item"
            android:text="ViewModel" />

        <TextView
            android:id="@+id/tv_live_date"
            style="@style/style_todo_item"
            android:text="LiveData" />

    </LinearLayout>

</androidx.core.widget.NestedScrollView>